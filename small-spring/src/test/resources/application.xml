<?xml version="1.0" encoding="UTF-8"?>
<beans>
    <bean id="userDao" name="dao" class="com.daihuaiyu.springframework.bean.UserDao"/>
    <bean id="userService" name="service" class="com.daihuaiyu.springframework.bean.UserService">
        <property name="userName" value="daihuaiyu"/>
        <property name="company" value="北京创联教育"/>
        <property name="location" value="北京市东大街"/>
        <property name="userDao" ref="userDao"/>
    </bean>
    <bean class="com.daihuaiyu.springframework.bean.MyBeanPostProcessor"/>
    <bean class="com.daihuaiyu.springframework.bean.MyBeanFactoryPostProcessor"/>
</beans>