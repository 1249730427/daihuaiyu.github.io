<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.IUserDao1">

    <select id="queryUserInfoById" parameterType="java.lang.Long" resultType="domain.User">
        SELECT id, name
        FROM user
        where id = #{id}
    </select>

    <select id="queryUserList" parameterType="domain.User" resultType="domain.User">
        SELECT id, name, age, createTime, updateTime
        FROM user
        where age = #{age}
    </select>

</mapper>